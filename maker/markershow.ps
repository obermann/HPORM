%!PS-Adobe-3.0

% Show errors, because GS PDF may not!
/..stopped /stopped load def
/stopped {
	$error /newerror get {
		handleerror
	} if
	..stopped
} bind odef

/..show /show load def
/show {
	dup TestMarker
	{
		dup Marker
		Char dup 0 4 -1 roll put
		//..show
	} forall
} bind odef

/..ashow /ashow load def
/ashow {
	dup TestMarker
	{
		dup Marker
		Char dup 0 4 -1 roll put
		3 copy
		//..ashow
		pop % Char
	} forall
	2 pops % Params
} bind odef

/..awidthshow /awidthshow load def
/awidthshow {
	dup TestMarker
	{
		dup Marker
		Char dup 0 4 -1 roll put
		6 copy
		//..awidthshow
		pop % Char
	} forall
	5 pops % Params
} bind odef

/..widthshow /widthshow load def
/widthshow {
	dup TestMarker
	{
		dup Marker
		Char dup 0 4 -1 roll put
		4 copy
		//..widthshow
		pop % Char
	} forall
	3 pops % Params
} bind odef

