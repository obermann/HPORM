		2 copy
		//..cshow
		pop % Char
	} forall
	1 pops % Params
} bind odef

/..xshow /xshow load def
/xshow {
	exch
	dup TestMarker
	0 1 2 index length 1 sub {
		dup 3 index exch 1 getinterval % array
		exch 2 index exch get % char int
		dup Marker
		Char dup 0 4 -1 roll put
		exch
		//..xshow
	} for
	2 pops
} bind odef

