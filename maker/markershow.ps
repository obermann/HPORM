% Unused in GS_PDF_ProcSet
/..xyshow /xyshow load def
/xyshow {
	exch
	dup TestMarker
	0 1 2 index length 1 sub {
		dup 3 index exch 2 mul 2 getinterval % array
		exch 2 index exch get % char int
		dup Marker
		Char dup 0 4 -1 roll put
		exch
		//..xyshow
	} for
	2 pops
} bind odef

